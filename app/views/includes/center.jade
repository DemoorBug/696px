    .mi_bc
      ul.mi_bc_ul
        -for(var i = rdjj.length-1; i >= 0; i--)
          li
            a(href='/movie/#{rdjj[i]._id}', title='#{rdjj[i].title}', target='_blank')
              span.mi_bc_img
                -if(rdjj[rdjj.length-1].poster)
                      img(src='/upload/'+rdjj[rdjj.length-1].poster)
                -else
                  img(src='/svg/iconhtml(#{parseInt(26*Math.random())}).svg')
              em #{rdjj[i].title}
              span.mi_bc_posi #{rdjj[i].describe}
