  .matter_fr_boxOne.Top
    .mi_cn.clearfix
      h3.mi_h3 #{categorys[13].name}
      .more
        a(href='/results?cat=#{categorys[13]._id}&p=0', target='_blank', style='margin-right:10px;')
          em.ko &#xE900;
          | 更多
    .mi_cc
      .mi_cc_ci
        ul.mi_ul
          -for(var i = xwrcb.length-1; i >= 0; i--)
            li
              case i+1
                when 1
                  i.fis #{i + 1}
                when 2
                  i.fss #{i + 1}
                when 3
                  i.fcs #{i + 1}
                default
                  i #{i + 1}
              span #{moment(xwrcb[i].meta.updateAt).format('MM.DD')}
              a(href='/movie/#{xwrcb[i]._id}', target='_blank', title='#{xwrcb[i].title}') #{xwrcb[i].title}
